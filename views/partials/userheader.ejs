    <script>
        document.getElementById('categorySearchForm').addEventListener('submit', function (e) {
            e.preventDefault();
            const categoryOptions = document.querySelectorAll('.select-active option');
        });

        function validateName(){
            var categorySearchInput = document.getElementById('categorySearchInput').value;

            document.getElementById('searchError').innerHTML = '';

            const nameRegex = /^[a-zA-Z\s]*$/;
            
            if(categorySearchInput==''||categorySearchInput.trim()==''){
                document.getElementById('searchError').innerHTML = 'Name is required.';
                setTimeout(function() {
                    document.getElementById('searchError').innerHTML = '';
                }, 5000);
                return false;
            }

            if(!categorySearchInput.match(nameRegex)){
                document.getElementById('searchError').innerHTML = 'Name is required.';
                setTimeout(function() {
                    document.getElementById('searchError').innerHTML = '';
                }, 5000);
                return false;
            }

            return true
        }
        
        function validateNames(){
            var categorySearchInput = document.getElementById('categorySearchInputs').value;

            document.getElementById('searchErrors').innerHTML = '';

            const nameRegex = /^[a-zA-Z\s]*$/;
            
            if(categorySearchInput==''||categorySearchInput.trim()==''){
